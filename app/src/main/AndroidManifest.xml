<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.note">

<uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launchersakib"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launchersakib_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Note">
        <activity android:name=".Notedetails"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"></activity>
        <activity android:name=".Editenoteactivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity
            android:name=".Createnote"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".notesActivity" />
        <activity android:name=".forgetpassword" />
        <activity android:name=".signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>